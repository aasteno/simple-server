body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif

a:link
  color: #00B7FF
  text-decoration: none
a:visited
  color: #B700FF
  text-decoration: none
a:hover
  color: #80DBFF
  text-decoration: underline
a:active
  color: #005C80
  text-decoration: underline

.nav
  float: right
  list-style-type: none
.active
  list-style-type: disc

.contact
  list-style-type: none

img
  vertical-align: middle
